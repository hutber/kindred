    "nyc": "^10.1.2",
    "resolve-url-loader": "^2.0.2",
    "url-loader": "^0.5.8",
    "webpack-config": "^7.0.0",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.2"
